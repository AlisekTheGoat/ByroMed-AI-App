generator client {
  provider = "prisma-client-js"
  output   = "./generated/neon"
}

datasource db {
  provider = "postgresql"
  url      = env("NEON_DATABASE_URL")
}

model Profile {
  authSub    String   @id @map("auth_sub")
  email      String?
  name       String?
  clinicName String?
  updatedAt  DateTime @default(now()) @map("updated_at")

  @@map("Profile")
}
