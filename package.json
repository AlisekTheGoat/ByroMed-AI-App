{
  "name": "byromed-ai-app",
  "version": "1.0.0",
  "description": "ByroMed AI â€” single-window single-agent Electron app",
  "main": "dist/electron/main.js",
  "scripts": {
    "dev": "concurrently -k -n VITE,MAIN -c magenta,cyan \"vite\" \"wait-on http://localhost:5173 && cross-env NODE_ENV=development node scripts/esbuild.mjs\"",
    "build": "vite build && cross-env NODE_ENV=production node scripts/esbuild.mjs --prod",
    "start": "electron .",
    "make": "npm run build && electron-builder"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlisekTheGoat/ByroMed-AI-App.git"
  },
  "bugs": {
    "url": "https://github.com/AlisekTheGoat/ByroMed-AI-App/issues"
  },
  "homepage": "https://github.com/AlisekTheGoat/ByroMed-AI-App#readme",
  "license": "ISC",
  "devDependencies": {
    "@types/node": "^22.0.0",
    "@types/react": "^18.2.74",
    "@types/react-dom": "^18.2.22",
    "@vitejs/plugin-react": "^5.0.0",
    "autoprefixer": "^10.4.20",
    "chokidar": "^4.0.3",
    "concurrently": "^9.2.0",
    "cross-env": "^10.0.0",
    "electron": "^31.2.0",
    "electron-builder": "^24.13.3",
    "esbuild": "^0.25.8",
    "postcss": "^8.4.41",
    "prisma": "^6.14.0",
    "tailwindcss": "^3.4.10",
    "typescript": "^5.5.4",
    "vite": "^7.1.2",
    "wait-on": "^8.0.4"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "uuid": "^11.1.0"
  },
  "build": {
    "appId": "com.byromed.ai",
    "asar": true,
    "files": [
      "dist/**",
      "agent/**",
      "package.json"
    ],
    "extraResources": [
      { "from": "agent", "to": "agent" }
    ],
    "mac": { "target": "dmg" },
    "win": { "target": "nsis" },
    "linux": { "target": "AppImage" }
  }
}
